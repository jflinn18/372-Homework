package HomeworkTracker;

import java.util.*;

public class HomeworkTracker {

	public List<Double> list = new ArrayList<Double>();
	public Iterator<Double> e;
	
	public double calcMin()
	{
		e = list.iterator();
		
		double min = 0;
		double d;
		
		while(e.hasNext())
		{
			d = e.next();
			
			if (min > d)
				min = d;
		}
		
		return min;
	}
	
	public double calcMax()
	{
		e = list.iterator();
		double max = 0;
		double d;
		
		while (e.hasNext())
		{
			d = e.next();
			if(max < d)
				max = d;
		}
		
		return max;
	}
	
	public double calcAve()
	{
		
	}
	
	public addScore(double d)
	{
		list.add(d);
	}
}
