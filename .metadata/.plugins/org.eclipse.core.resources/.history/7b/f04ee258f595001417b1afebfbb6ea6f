package ex2_3;

public class DataOnly {

	// member variables
	public static int i;
	public static float f;
	public static boolean b;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// new DataOnly object
		DataOnly d = new DataOnly();
		
		// prints out the member variables
		System.out.printf("i is %d, f is %f, b is %b", geti(), getf(), getb());
	}
	
	public static int geti(){return i;}
	public static float getf(){return f;}
	public static boolean getb(){return b;}
	
	// allocation of member variables
	public static void seti(){ i = 47; }
	public static void setf(){ f = 1.1f;}
	public static void setb(){ b = false;}
}
